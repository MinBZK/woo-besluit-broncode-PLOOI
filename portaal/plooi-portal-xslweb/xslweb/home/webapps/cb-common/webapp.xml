<?xml version="1.0"?>
<webapp 
  xmlns="http://www.armatiek.com/xslweb/webapp"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.armatiek.com/xslweb/webapp ../../config/xsd/xslweb/webapp.xsd">
  
  <title>Gemeenschappelijke resources van KOOP componenten bibliotheek</title>
  <description>Gemeenschappelijke resources van KOOP componenten bibliotheek</description>
  <development-mode>false</development-mode>
  <max-upload-size>1</max-upload-size>
  
  <!-- Resources to serve straight away: -->
  <resources> 
    <resource pattern="/favicon.ico" media-type="image/x-icon" duration="P7DT0H0M0S"/>
    <resource pattern="/robots.txt" media-type="text/plain" duration="P1DT0H0M0S"/> 
    <resource pattern=".+\.png$" media-type="image/png"/>
    <resource pattern=".+\.gif$" media-type="image/gif"/>
    <resource pattern=".+\.(jpg|jpeg)$" media-type="image/jpg"/>
    <resource pattern=".+\.svg$" media-type="image/svg+xml"/>
    <resource pattern=".+\.ico$" media-type="image/vnd.microsoft.icon"/>
    <resource pattern=".+\.js$" media-type="text/javascript" duration="P0DT0H0M10S"/>
    <resource pattern=".+\.css$" media-type="text/css" duration="P0DT0H0M10S"/> 
    <resource pattern=".+\.(ttf|woff|eot|html)$" media-type="application/octet-stream"/>
  </resources>
  
</webapp>